<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.adrian.appe">

    <uses-permission android:name="android.permission.CAMERA"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <protected-broadcast android:name="android.intent.action.MEDIA_MOUNTED"></protected-broadcast>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Inicio">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FormularioGeneral"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".Ingresa"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".Web" />
        <activity android:name=".FormularioGeneral2"
            android:windowSoftInputMode="stateHidden">

        </activity>
       </application>

</manifest>